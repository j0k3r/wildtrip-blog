---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>My little world wide tour</title>
  <link href="http://blog.wildtrip.net/rss.xml" rel="self"/>
  <link href="http://blog.wildtrip.net/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://blog.wildtrip.net/</id>
  <author>
    <name>Jeremy</name>
    <email>jeremy.benoist@gmail.com</email>
  </author>

  {% for post in site.posts limit:5 %}
  <entry>
    <title><![CDATA[{{ post.title }}]]></title>
    <link href="{{ post.posterous_url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>{{ post.posterous_url }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}

</feed>
